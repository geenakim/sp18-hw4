<div>
	<h1>Cats</h1>
	<% @cats.each do |cat|%>
		<%= cat.name%>
	<% end %>

	<h1>Users</h1>
	<% @users.each do |user|%>
		<%= user.username%>
		<%= user.email%>
		<%= user.age%>
	<% end %>

	<h1>Todos</h1>
	<% @todos.each do |todo|%>
		<%= check_box_tag('finished', todo.id, todo.finished)%>
		<%= todo.tasks%>
	<% end %>
	<%= link_to 'Add task', '/todos/new' %>

</div>